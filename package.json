{
  "name": "node-typescript-jest-eslint-boilerplate",
  "version": "1.0.0",
  "author": "yantene",
  "license": "CC0-1.0",
  "scripts": {
    "start": "node ./dist/start.js",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "lint": "run-s lint:*",
    "lint:prettier": "prettier --check \"./**/*.ts\"",
    "format": "run-s format:*",
    "format:prettier": "prettier --write \"./**/*.ts\"",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
